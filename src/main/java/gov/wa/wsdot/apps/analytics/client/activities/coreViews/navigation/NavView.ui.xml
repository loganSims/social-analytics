<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:m.html="urn:import:gwt.material.design.client.ui.html">

<g:HTMLPanel>
    <m:MaterialNavBar ui:field="navBar" backgroundColor="teal lighten-2" width="100%" type="FIXED">
        <m:MaterialImage ui:field="logo"/>
        <m:MaterialNavSection float="RIGHT">
            <m:MaterialLink ui:field="home" iconType="HOME" iconPosition="LEFT" textColor="white" text="Dashboard" />
            <m:MaterialLink ui:field="search" iconType="SEARCH" iconPosition="LEFT" textColor="white" text="Search" />
        </m:MaterialNavSection>
    </m:MaterialNavBar>
</g:HTMLPanel>
</ui:UiBinder>